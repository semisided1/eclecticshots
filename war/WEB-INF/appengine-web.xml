<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>eclecticshots</application>
  <version>1</version>

  <!--
    By default, App Engine sends requests serially to a given web server.
    To allow App Engine to send multiple requests in parallel specify:

      <threadsafe>true</threadsafe>
  -->
 
  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
  </system-properties>
  <static-files>
  	<include path="/css/eclecticshots.css"></include>
  	<include path="/img/building.jpg"></include>
  	<include path="/img/wall.jpg"></include>
  		<include path="/img/shelly.jpg"></include>
  		<include path="/img/butterfly.png"></include>
  		<include path="/fbook.html"></include>
  		<include path="/booty.html"></include>
  		<include path="/img/architecture.jpg"></include>
  		<include path="/favicon.ico"></include>
  		<include path="/css/main.css"></include>
  	</static-files></appengine-web-app>
